<style>
    h1, h2, h3, h4, p {
        color: white
    }
</style>
<div id="AllCarsDivId" class="row">
   
</div>
<input type="button" id="CarLoadBtn" value="Load cars"/>
@section Scripts
{
    <script>
        var objectToPass = { "IsNew": true }
            $('#CarLoadBtn').click(function () {
                $.ajax({
                    type: 'POST',
                    url: '@Url.Content("~/Car/GetCarPartial")',
                    data: objectToPass,
                    success: function (data) {
                        $('#AllCarsDivId').html(data);
                    },
                    error: function (data) { alert("error") }
                });
            });
    </script>
}
